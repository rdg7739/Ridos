<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="#FF2E8050">

    <ImageView
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:id="@+id/imageView10"
        android:background="@drawable/profile_welcome_001"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageView"
        android:layout_toEndOf="@+id/imageView"
        android:layout_marginTop="20dp" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:id="@+id/idText"
        android:background="@drawable/login_001_button"
        android:focusableInTouchMode="true"
        android:hint="ID"
        android:editable="true"
        android:paddingLeft="10dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/imageView10" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:id="@+id/pswd"
        android:background="@drawable/login_001_button"
        android:layout_below="@+id/idText"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="8dp"
        android:hint="Password"
        android:focusableInTouchMode="true"
        android:editable="true"
        android:paddingLeft="10dp"
        android:password="true" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:id="@+id/pswdConfirm"
        android:background="@drawable/login_001_button"
        android:layout_below="@+id/pswd"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="8dp"
        android:hint="Confirm Password"
        android:focusableInTouchMode="true"
        android:editable="true"
        android:paddingLeft="10dp"
        android:password="true" />


    <EditText
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:id="@+id/name"
        android:background="@drawable/login_001_button"
        android:layout_below="@+id/pswdConfirm"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="8dp"
        android:hint="Name"
        android:focusableInTouchMode="true"
        android:editable="true"
        android:paddingLeft="10dp"
        android:password="true" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:id="@+id/phone"
        android:background="@drawable/login_001_button"
        android:hint="Phone #"
        android:focusableInTouchMode="true"
        android:editable="true"
        android:paddingLeft="10dp"
        android:phoneNumber="true"
        android:layout_below="@+id/name"
        android:layout_toRightOf="@+id/imageView4"
        android:layout_marginTop="8dp" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:id="@+id/address"
        android:background="@drawable/login_001_button"
        android:hint="Address"
        android:focusableInTouchMode="true"
        android:editable="true"
        android:paddingLeft="10dp"
        android:layout_below="@+id/phone"
        android:layout_toRightOf="@+id/imageView4"
        android:layout_marginTop="8dp" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:id="@+id/imageView"
        android:background="@drawable/profile_001_deco"
        android:layout_above="@+id/pswd"
        android:layout_toLeftOf="@+id/idText"
        android:layout_toStartOf="@+id/idText"
        android:layout_marginLeft="8dp" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:id="@+id/imageView2"
        android:background="@drawable/profile_001_deco"
        android:layout_alignTop="@+id/pswd"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:id="@+id/imageView11"
        android:background="@drawable/profile_001_deco"
        android:layout_alignTop="@+id/pswdConfirm"
        android:layout_alignRight="@+id/imageView"
        android:layout_alignEnd="@+id/imageView" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:id="@+id/imageView3"
        android:background="@drawable/profile_001_deco"
        android:layout_alignTop="@+id/name"
        android:layout_alignRight="@+id/imageView2"
        android:layout_alignEnd="@+id/imageView2" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:id="@+id/imageView4"
        android:background="@drawable/profile_001_deco"
        android:layout_alignTop="@+id/phone"
        android:layout_alignRight="@+id/imageView3"
        android:layout_alignEnd="@+id/imageView3" />

    <ImageView
        android:layout_width="40dp"
        android:layout_height="30dp"
        android:id="@+id/imageView5"
        android:background="@drawable/profile_001_deco"
        android:layout_alignTop="@+id/address"
        android:layout_alignRight="@+id/imageView4"
        android:layout_alignEnd="@+id/imageView4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="submit"
        android:id="@+id/submitBtn"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="#00ffffff"
        android:textColor="#ffffff"
        android:stateListAnimator="@null"
        android:textSize="20dp" />


</RelativeLayout>